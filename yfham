-- ===== WEBHOOK DISCORD ANDA =====
local WEBHOOK_URL = "https://discord.com/api/webhooks/1461393962618060957/JAb0w8aEoWOV77_RCjN0fDhGJU5cbic6ZJQqlPL2JMzMztSQfFckuanMbYMXLTGdnNBN"
